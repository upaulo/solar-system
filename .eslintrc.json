{
	"env": {
		"browser": true,
		"es2020": true
	},
	"extends":[
		"airbnb",
		"airbnb-typescript"
	],
	"ignorePatterns": ["dist", "vite.config.ts"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "./tsconfig.json"
	},
	"rules": {
		"react/jsx-uses-react": "off",
		"react/react-in-jsx-scope": "off",
		"no-console": "off"
	}
}
